{% extends "singles/single.twig" %}

{% block before_header_content %}
{% endblock %}

{% block after_header_content %}
{% endblock %}

{% block content %}
	
	{% if post.video %}
		<h3>Video</h3>
		{{post.video}}
	{% endif %}

	{% if post.slides %}
		<h3>Slides</h3>
		{{post.slides}}
	{% endif %}
	
	{% for block in post.get_field('blocks') %} 
		{% include 'blocks/block-logic.twig' %}
	{% endfor %}

{% endblock %}